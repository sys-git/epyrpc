<masterLauncher
    xmlns="masterLauncher"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="masterLauncher validation/masterLauncher.xsd">
    <Configurations>
        <heads>    
            <Cmdline>
                <enabled>true</enabled>
                <launchType>thread</launchType>
                <api>
                    <name>EO_V1</name>
                    <maxConcurrentCalls>1</maxConcurrentCalls>
                </api>
                <args>
                    <clspath>commandline.Controller.ExampleCmdline</clspath>
                    <cls>ExampleCmdline</cls>
                </args>
            </Cmdline>
            <GUI_QT4>
                <enabled>false</enabled>
                <launchType>thread</launchType>
                <api>
                    <name>EO_V1</name>
                    <maxConcurrentCalls>1</maxConcurrentCalls>
                </api>
                <args>
                    <resourceRoot>GUI/Qt4/BusinessLogic/Screens/xml</resourceRoot>
                    <assetRoot>UI/DebugGUI/screens/assets</assetRoot>
                    <filename>example.ui</filename>
                    <mainWindow>
                        <clspath>GUI.Qt4.BusinessLogic.Screens.impls.ExampleUI</clspath>
                        <cls>ExampleUI</cls>
                    </mainWindow>
                </args>
            </GUI_QT4>
        </heads>
        <body>
            <ExecutionOrganiser>
                <launchType>thread</launchType>
                <api>
                    <name>EO_V1__HANDLER</name>
                    <maxConcurrentCalls>10</maxConcurrentCalls>
                </api>
            </ExecutionOrganiser>
        </body>
        <api>
            <namespace>YouView</namespace>
            <ipc>
                <type>MQUEUE</type>
            </ipc>
        </api>
    </Configurations>
</masterLauncher>
