<resultWorker
    xmlns="resultWorker"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="resultWorker validation/resultWorker.xsd">
    <output>
	    <fileSystem enabled="false">
	        <location>/tmp/tas/results</location>
	        <clear>true</clear>
            <archive>
                <name>resultsPackage</name>
                <format>tar.gz</format>
                <location>/tmp/results</location>
            </archive>
	    </fileSystem>
        <sqlLite enabled="false">
            <location>./result.sqlite3</location>
            <clear>true</clear>
        </sqlLite>
        <trmsLoggers>
            <trms enabled="true">
                <url>https://devint-trms/trms</url>
                <username>blah</username>
                <password>blah</password>
            </trms>
        </trmsLoggers>
    </output>
    <customLogFilters></customLogFilters>
</resultWorker>
