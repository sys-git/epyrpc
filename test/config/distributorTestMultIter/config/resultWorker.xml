<resultWorker
    xmlns="resultWorker"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="resultWorker validation/resultWorker.xsd">
    <output>
        <fileSystem enabled="true">
            <location>/tmp/results</location>
            <clear>true</clear>
            <archive>
                <name>resultsPackage</name>
                <format>tar.gz</format>
                <location>/tmp/results</location>
            </archive>
        </fileSystem>
        <sqlLite enabled="true">
            <location>/tmp/results/result.sqlite3</location>
            <clear>true</clear>
        </sqlLite>
        <trmsLoggers>
            <trms enabled="false">
                <url>https://devint-trms/trms</url>
                <username>blah</username>
                <password>blah</password>
            </trms>
            <trms enabled="false">
                <url>https://devint-trms/trms</url>
                <username>blah</username>
                <password>blah</password>
            </trms>
        </trmsLoggers>
    </output>
    <customLogFilters>
        <customLogFilter>
            <location>/tmp/blah</location>
            <filters>
                <filter type="literal">result.log</filter>
            </filters>
        </customLogFilter> 
        <customLogFilter>
            <location>/tmp</location>
            <filters>
                <filter type="regex">customLog[0-9]+\.log</filter>
                <filter type="literal">blah.log</filter>
            </filters>
        </customLogFilter> 
    </customLogFilters>
</resultWorker>
